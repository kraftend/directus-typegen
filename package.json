{
  "name": "@kraftend/directus-typegen",
  "version": "1.0.0",
  "description": "a Directus helper that generates typescript types for your instance.",
  "files": ["dist", "package.json"],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "directus-typegen": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kraftend/directus-typegen.git"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "keywords": [],
  "author": "Cemre Inanc <cemreinanc@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@directus/shared": "^9.24.0",
    "@types/fs-extra": "^11.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "axios": "^1.5.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1"
  }
}
